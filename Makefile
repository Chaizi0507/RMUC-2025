JLINK_SCRIPT = jlink_script.jlink
DEVICE = STM32F427II
INTERFACE = SWD
SPEED = 4000

download: $(JLINK_SCRIPT)
	@echo "Downloading firmware to target using JLink..."
	$(JLINK) -device $(DEVICE) -autoconnect 1 -if $(INTERFACE) -speed $(SPEED) -CommanderScript $(JLINK_SCRIPT)

