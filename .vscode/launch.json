{
    "version": "0.2.0",
    "configurations": [
        // 使用j-link进行调试时的参考配置
        {
            "name": "Jlink",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}\\build\\M57-Dart.elf",
            "request": "launch",
            "type": "cortex-debug",
            "device": "Cortex-M4",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "svdFile": "STM32F427.svd",
            "rtos": "FreeRTOS",
            //"preLaunchTask": "build task",//先运行Build任务,取消注释即可使用
            "preLaunchTask": "download task",//调试完成后运行download任务,取消注释即可使用
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            }
        }

    ]
}
